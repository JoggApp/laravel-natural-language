{
    "name": "joggapp/laravel-natural-language",
    "description": "Laravel package for the Google Natural language API",
    "keywords": [
        "laravel",
        "package",
        "google",
        "natural language",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Harish Toshniwal",
            "email": "harish@jogg.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoggApp\\NaturalLanguage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoggApp\\NaturalLanguage\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "google/cloud-language": "^0.28.0",
        "illuminate/support": "10.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench" : "8.*",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoggApp\\NaturalLanguage\\NaturalLanguageServiceProvider"
            ],
            "aliases":{
                "NaturalLanguage": "JoggApp\\NaturalLanguage\\NaturalLanguageFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
